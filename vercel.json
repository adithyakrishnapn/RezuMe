{
    "version":2,
    "builds": [
        {
            "src" :"./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/createpdf(.*)", "dest": "/createpdf" },
        { "src": "/about(.*)", "dest": "/about" },
        { "src": "/signup(.*)", "dest": "/signup" },
        { "src": "/(.*)", "dest": "/" }
    ]
}