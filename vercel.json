{
    "builds": [
      {
        "src": "/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "routes/index.js"
      },
      {
        "src": "/about",
        "dest": "/routes/about.js"
      },
      {
        "src": "/createpdf",
        "dest": "routes/createpdf.js"
      },
      {
        "src": "/signup",
        "dest": "routes/signup.js"
      }
    ]
  }
  